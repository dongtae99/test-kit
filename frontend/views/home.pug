    style.
      /* 스타일 조정 */
      #menu_wrap {
        position: relative;
        text-align: center; /* 텍스트 및 버튼 중앙 정렬 */
        margin: 0;
        top: 0px;
        left: 0px;
        z-index: 10;
        background-color: rgb(255, 255, 255);
        padding: 5px;
        border-radius: 5px;
        box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);
      }
      #form_wrap {
        position: relative;
        text-align: center; /* 텍스트 및 버튼 중앙 정렬 */
        margin: 0;
        top: 30px;
        left: 120px;
        z-index: 10;
        background-color: rgb(255, 255, 255, 0);
        padding: 5px;
        border-radius: 5px;
      }
      .title-box {
        background-color:rgb(255, 189, 113); /* 박스 배경색 */
        padding: 80px;            /* 박스 내부 여백 */
        border-radius: 0px;       /* 모서리 둥글게 */
        border: 0px solid #ccc;   /* 테두리 추가 */
        margin-bottom: 0px;      /* 아래 여백 */
        max-width: 1140px;
        margin: 0 auto;

      }

  body(style="background-color: rgb(254, 241, 214); ")
          .title-box
